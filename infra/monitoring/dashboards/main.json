{
  "dashboard": {
    "title": "IntelliFlow CRM - Main Dashboard",
    "tags": ["intelliflow", "crm", "overview"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "API Request Rate",
        "targets": [
          {
            "expr": "rate(intelliflow_api_request_count[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ],
        "yaxes": [
          { "label": "Requests/sec", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 2,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "type": "graph",
        "title": "API Response Time (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(intelliflow_api_request_duration_bucket[5m]))",
            "legendFormat": "{{endpoint}} p95"
          }
        ],
        "yaxes": [
          { "label": "Latency (ms)", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 3,
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 },
        "type": "stat",
        "title": "Total Leads",
        "targets": [
          {
            "expr": "sum(intelliflow_lead_created)"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 8 },
        "type": "stat",
        "title": "Converted Leads",
        "targets": [
          {
            "expr": "sum(intelliflow_lead_converted)"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      },
      {
        "id": 5,
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
        "type": "stat",
        "title": "Conversion Rate",
        "targets": [
          {
            "expr": "(sum(intelliflow_lead_converted) / sum(intelliflow_lead_created)) * 100"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "colorMode": "value",
          "unit": "percent"
        }
      },
      {
        "id": 6,
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
        "type": "stat",
        "title": "Error Rate",
        "targets": [
          {
            "expr": "(rate(intelliflow_api_error_count[5m]) / rate(intelliflow_api_request_count[5m])) * 100"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value",
          "colorMode": "value",
          "unit": "percent"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "value": 0, "color": "green" },
            { "value": 1, "color": "yellow" },
            { "value": 5, "color": "red" }
          ]
        }
      },
      {
        "id": 7,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "type": "graph",
        "title": "Lead Score Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(intelliflow_lead_score_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(intelliflow_lead_score_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(intelliflow_lead_score_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          { "label": "Score", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 8,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "type": "graph",
        "title": "AI Inference Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(intelliflow_ai_inference_latency_bucket[5m]))",
            "legendFormat": "{{model}} p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(intelliflow_ai_inference_latency_bucket[5m]))",
            "legendFormat": "{{model}} p95"
          }
        ],
        "yaxes": [
          { "label": "Latency (ms)", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "type": "graph",
        "title": "Database Query Performance",
        "targets": [
          {
            "expr": "rate(intelliflow_db_query_count[5m])",
            "legendFormat": "{{operation}} {{table}}"
          }
        ],
        "yaxes": [
          { "label": "Queries/sec", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 10,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "type": "graph",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(intelliflow_cache_hit[5m]) / (rate(intelliflow_cache_hit[5m]) + rate(intelliflow_cache_miss[5m]))",
            "legendFormat": "Hit Rate"
          }
        ],
        "yaxes": [
          { "label": "Hit Rate", "show": true, "min": 0, "max": 1 },
          { "show": false }
        ]
      },
      {
        "id": 11,
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 32 },
        "type": "graph",
        "title": "Active Users",
        "targets": [
          {
            "expr": "intelliflow_users_active"
          }
        ],
        "yaxes": [
          { "label": "Users", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 12,
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 32 },
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "intelliflow_system_memory_usage / 1024 / 1024"
          }
        ],
        "yaxes": [
          { "label": "MB", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 13,
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 32 },
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "intelliflow_system_cpu_usage"
          }
        ],
        "yaxes": [
          { "label": "Percentage", "show": true, "min": 0, "max": 100 },
          { "show": false }
        ]
      },
      {
        "id": 14,
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 40 },
        "type": "table",
        "title": "Top 10 Slowest Endpoints",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, rate(intelliflow_api_request_duration_bucket[5m])))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "endpoint": "Endpoint",
                "method": "Method",
                "Value": "P95 Latency (ms)"
              }
            }
          }
        ]
      },
      {
        "id": 15,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 48 },
        "type": "graph",
        "title": "AI Inference Cost",
        "targets": [
          {
            "expr": "sum(rate(intelliflow_ai_inference_cost[1h])) by (model)",
            "legendFormat": "{{model}}"
          }
        ],
        "yaxes": [
          { "label": "Cost (USD/hour)", "show": true },
          { "show": false }
        ]
      },
      {
        "id": 16,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 48 },
        "type": "graph",
        "title": "Opportunity Pipeline Value",
        "targets": [
          {
            "expr": "sum(intelliflow_opportunity_value) by (stage)",
            "legendFormat": "{{stage}}"
          }
        ],
        "yaxes": [
          { "label": "Value (USD)", "show": true },
          { "show": false }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(intelliflow_system_memory_usage[1m]) > 0",
          "iconColor": "blue",
          "tagKeys": "deployment"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(intelliflow_api_request_count, environment)",
          "current": {
            "value": "production",
            "text": "production"
          },
          "refresh": 1
        },
        {
          "name": "service",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(intelliflow_api_request_count, service)",
          "current": {
            "value": "all",
            "text": "All"
          },
          "includeAll": true,
          "refresh": 1
        }
      ]
    }
  },
  "overwrite": true,
  "folderUid": "intelliflow-crm",
  "message": "IntelliFlow CRM main dashboard - Auto-generated"
}
